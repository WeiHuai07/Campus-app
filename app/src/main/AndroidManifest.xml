<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.student">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name="com.android.student.App.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/origin"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.android.student.Activity.MainActivity" />
        <activity android:name="com.android.student.Activity.ActActivity" />
        <activity android:name="com.android.student.Activity.YyActivity" />
        <activity android:name="com.android.student.Activity.StudentChangeActivity"
            android:label="修改个人信息"/>
        <activity android:name="com.android.student.Activity.AddCourseActivity"
            android:label="修改课程信息"/>
        <activity android:name="com.android.student.Activity.ModifyPasswdActivity"
            android:label="修改个人密码"/>
        <activity android:name="com.android.student.App.LoginActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>